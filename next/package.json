{
  "$schema": "https://json.schemastore.org/jsconfig",
  "name": "id-game-distribution-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run move",
    "build-test": "next build && npm run move-test",
    "build-old-dev": "next build && npm run move-old-dev",
    "build-old": "next build && npm run move-pro",
    "build-only": "next build",
    "start": "next start",
    "server": "node .next/standalone/server.js",
    "lint": "next lint",
    "move": "cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public",
    "move-test": "xcopy /e /i .next\\static .next\\standalone\\.next\\static && xcopy /e /i public .next\\standalone\\public",
    "move-old-dev": "xcopy /e /i .next\\static .next\\standalone\\.next\\static",
    "move-old": "cp -r .next/static .next/standalone/.next/static"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "animate.css": "^4.1.1",
    "antd": "^5.20.0",
    "blueimp-md5": "^2.19.0",
    "bootstrap": "^3.4.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "firebase": "^10.12.2",
    "framer-motion": "^11.3.28",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "js-base64": "^3.7.7",
    "next": "^13.5.6",
    "react": "^18.3.1",
    "react-cookie": "7.1.4",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.0",
    "react-slick": "^0.30.2",
    "sass": "^1.77.8",
    "sharp": "^0.33.4",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.18.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "esbuild-loader": "^4.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
